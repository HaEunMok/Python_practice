  a,b,c =input(1 2 3).split(".")
  print(a,b,c) 1 2 3
  
   a,c=input().split("-")
  print(a+c)
 
 
  -sep
 print(a,b,c,sep="-")
 a-b-c
 
 y,m,d=input().split(".")
print(d, m, y,sep="-")

 
 
